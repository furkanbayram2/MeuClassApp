                                    <a href="javascript:void(0);">Something else here</a>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </div>
                <div class="body">

                    @using (Html.BeginForm("DoRegister", "AdminUser", FormMethod.Post, new { @class = "üyeFiltreleme " })) {
                    <div class="col-sm-4">
                        <div class="form-group">
                            <div class="form-line">
                                <input type="text" id="isimArama" class="form-control" placeholder="İsim" />
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-4">
                        <div class="form-group">
                            <div class="form-line">
                                <input type="text" id="soyisimArama" class="form-control" placeholder="Soyisim" />
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-4">
                        <div class="form-group">
                            <div class="form-line">
                                <select class="form-control show-tick" id="yetkiArama">
                                    <option value="">-- Yetki Seçiniz --</option>
                                    <option value="10">Öğrenci</option>
                                    <option value="20">Öğretmen</option>
                                    <option value="30">Admin</option>
                                </select>
                            </div>
                        </div>
                    </div>
                    <button type="button" class="btn btn-primary m-t-15 waves-effect">Arama Yap</button>
                    }

                </div>
            </div>

